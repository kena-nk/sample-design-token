{
  "global": {
    "colors": {
      "black": {
        "value": "#000",
        "type": "color"
      },
      "white": {
        "value": "#fff",
        "type": "color"
      }
    },
    "xs": {
      "value": "2",
      "type": "spacing",
      "description": "最小"
    },
    "sm": {
      "value": "4",
      "type": "spacing",
      "description": "小"
    },
    "lg": {
      "value": "8",
      "type": "spacing",
      "description": "大"
    },
    "xl": {
      "value": "16",
      "type": "spacing",
      "description": "最大"
    }
  },
  "dark": {
    "theme": {
      "fg": {
        "value": "{colors.white}",
        "type": "color"
      },
      "bg": {
        "value": "{colors.black}",
        "type": "color"
      }
    }
  },
  "light": {
    "theme": {
      "fg": {
        "value": "{colors.black}",
        "type": "color"
      },
      "bg": {
        "value": "{colors.white}",
        "type": "color"
      }
    },
    "borderRadius": {
      "base": {
        "value": "4",
        "type": "borderRadius"
      },
      "default": {
        "value": "{borderRadius.base} * 2",
        "type": "borderRadius"
      }
    }
  },
  "$themes": []
}